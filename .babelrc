{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        "styled-jsx/babel",
        ["react-docgen"],
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        "styled-jsx/babel",
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": false
          }
        ]
      ]
    }
  }
}
